using System;

namespace KeepRandomizing
{
    class Program
    {
        static Random r = new Random();
        static void Main(string[] args)
        {
            int n = r.Next(10, 500);

            for(int i=0; i<=n; i++)
            {
                int px = r.Next(0, 500);
                int py = r.Next(0, 500);

                Console.WriteLine("coordenadas: {0}, {1}", px, py);
            }
        }
    }
}
